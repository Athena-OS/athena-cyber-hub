<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="AthenaAchContainer" parent="AdwActionRow">
    <child type="suffix">
      <object class="GtkButton" id="btn_shell">
        <property name="icon-name">utilities-terminal-symbolic</property>
        <property name="tooltip-text" translatable="yes">Run container</property>
        <property name="valign">center</property>
      </object>
    </child>
    <child type="suffix">
      <object class="GtkButton" id="btn_init">
        <property name="icon-name">tab-new-symbolic</property>
        <property name="tooltip-text" translatable="yes">Create and run container</property>
        <property name="valign">center</property>
      </object>
    </child>
    <child type="suffix">
      <object class="GtkButton" id="btn_delete">
        <property name="icon-name">edit-delete-symbolic</property>
        <property name="tooltip-text" translatable="yes">Delete container</property>
        <property name="valign">center</property>
      </object>
    </child>
    <child type="suffix">
      <object class="GtkButton" id="btn_readme">
        <property name="icon-name">document-revert-rtl-symbolic</property>
        <property name="tooltip-text" translatable="yes">Read README</property>
        <property name="valign">center</property>
      </object>
    </child>
  </template>
</interface>
